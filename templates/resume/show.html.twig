{% extends 'base.html.twig' %}

{% block title %}Resume{% endblock %}

{% block body %}
    <h1>Resume</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resume.id }}</td>
            </tr>
            <tr>
                <th>Summary</th>
                <td>{{ resume.summary }}</td>
            </tr>
            <tr>
                <th>Datenaissance</th>
                <td>{{ resume.datenaissance ? resume.datenaissance|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ resume.adresse }}</td>
            </tr>
            <tr>
                <th>Facebook</th>
                <td>{{ resume.facebook }}</td>
            </tr>
            <tr>
                <th>Google</th>
                <td>{{ resume.google }}</td>
            </tr>
            <tr>
                <th>Twitter</th>
                <td>{{ resume.twitter }}</td>
            </tr>
            <tr>
                <th>Instagram</th>
                <td>{{ resume.instagram }}</td>
            </tr>
            <tr>
                <th>Slogan</th>
                <td>{{ resume.slogan }}</td>
            </tr>
            <tr>
                <th>Linkedin</th>
                <td>{{ resume.linkedin }}</td>
            </tr>
            <tr>
                <th>Mobile</th>
                <td>{{ resume.mobile }}</td>
            </tr>
            <tr>
                <th>Entreprise</th>
                <td>{{ resume.entreprise }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('resume_index') }}">back to list</a>

    <a href="{{ path('resume_edit', {'id': resume.id}) }}">edit</a>

    {{ include('resume/_delete_form.html.twig') }}
{% endblock %}
